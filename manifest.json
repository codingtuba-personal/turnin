{
    "name": "Turn In",
    "version": "0.0.1",
    "manifest_version": 3,
    "content_scripts":[
        {
            "matches": ["https://docs.google.com/document/d/*","https://docs.google.com/presentation/d/*"],
            "js": ["script.js"]
        },
        {
            "matches":["https://classroom.google.com/*"],
            "js": ["classroom.js"]
        }
    ],
    "icons":{ 
        "16": "icon.jpg",
        "48": "icon.jpg",
       "128": "icon.jpg" 
    }
}